@using MixERP.Purchases.Extensions
@model dynamic

@{
    ViewBag.Title = I18N.PurchaseReturns;
    Layout = ViewBag.SalesLayoutPath;
}

@Html.FinancePartialView("Shared/SearchView.cshtml", TenantConvention.GetTenant())


<script src="/Areas/MixERP.Purchases/scripts/tasks/return/index.js"></script>

<script>
    var annotationStatus = window.Enumerable.From(window.model.Annotation).Where(function(x) {
        return x.Id === "Status";
    }).FirstOrDefault();

    annotationStatus.DefaultValue = "Unverified";
    window.model.HasVerification = true;
    window.model.Title = window.translate("PurchaseReturnVerification");

    prepareView(model);
</script>

